{
	"name": "air-react-forms",
	"description": "A react library for forms validation",
	"author": "RolRom",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/arimorc/air-react-forms/"
	},
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"source": "src/index.js",
	"files": [
		"./dist"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"dependencies": {
		"@babel/runtime": "^7.13.10",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"prop-types": "^15.7.2",
		"react-scripts": "4.0.2"
	},
	"scripts": {
		"start": "npm-run-all --parallel build-watch start-playground",
		"build": "rollup -c",
		"build-watch": "rollup -c -w",
		"start-playground": "cd playground && yarn start",
		"install-all": "yarn install && cd playground && yarn install",
		"test": "jest",
		"eslint": "eslint --color --ext js,jsx src/ tests/ playground/src/",
		"release": "yarn run build && np"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.12.17",
		"@babel/core": "^7.12.17",
		"@babel/plugin-transform-runtime": "^7.13.15",
		"@babel/preset-env": "^7.12.17",
		"@babel/preset-react": "^7.12.13",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
		"enzyme": "^3.11.0",
		"enzyme-to-json": "^3.6.1",
		"eslint": "^7.18.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"jest-prop-type-error": "^1.1.0",
		"np": "^7.4.0",
		"npm-run-all": "^4.1.5",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"rollup": "^2.39.0",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-extensions": "^0.1.0",
		"rollup-plugin-peer-deps-external": "^2.2.4"
	},
	"peerDependencies": {
		"react": "^16.8.3 || ^17.0.0-0"
	},
	"license": "MIT",
	"keywords": [
		"react",
		"forms",
		"react-forms",
		"rolrom"
	]
}
